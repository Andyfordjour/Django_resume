{
    "builds": [{
        "src": "Django_resume/wsgi.py", 
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Django_resume/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
          },
          {
            "src": "/media/(.*)",
            "dest": "/media/$1"
          },
          {
            "src": "/(.*)",
            "dest": "manage.py"
          }
    ]
}